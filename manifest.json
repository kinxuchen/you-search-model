{
  "name": "you-search-model",
  "version": "0.0.1",
  "description": "通过 url 参数修改 you 搜索中的 Model",
  "manifest_version": 3,
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://you.com/*", "https://www.phind.com/*"],
      "run_at": "document_start",
      "js": ["src/content.tsx"]
    }
  ],
  "background": {
    "service_worker": "src/background.ts"
  },
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "https://you.com/*",
    "https://www.phind.com/*"
  ]
}
